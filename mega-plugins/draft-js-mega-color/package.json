{
  "name": "draft-js-mega-color-plugin",
  "version": "1.0.0",
  "description": "MegaDraft Color Plugin for DraftJS",
  "authors": [
    "Benjamin Kniffler <benjamin@kniffler.com> (https://github.com/bkniffler)",
    "Nik Graf <nik@nikgraf.com> (http://www.nikgraf.com)",
    "Steven Iseki <stevenisekimartin@gmail.com> (https://github.com/StevenIseki)",
    "Marcelo Jorge Vieira <metal@alucinados.com> (https://github.com/marcelometal)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vacenz/megadraft-js-plugins.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "scripts": {
    "clean": "../node_modules/.bin/rimraf lib",
    "build": "npm run clean && npm run build:js",
    "build:js": "WEBPACK_CONFIG=$(pwd)/webpack.config.js BABEL_DISABLE_CACHE=1 BABEL_ENV=production NODE_ENV=production ../node_modules/.bin/babel --out-dir='lib' --ignore='__test__/*' src",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "react": ">=15.3.1",
    "react-dom": ">=15.3.1"
  }
}
